{
  "name": "Spotify Search Plus",
  "description": "Search for songs, albums and artists without installing Spotify or having an account.",
  "version": "2.9",
  "omnibox": { "keyword" : "spotify" },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "images/logo19.png"
  },
  "icons":{
	  "16":"images/logo16.png",
	  "48":"images/logo48.png",
	  "128":"images/logo128.png"
  },
  "permissions": [
    "contextMenus",
	"http://ws.spotify.com/"
  ],
  "manifest_version": 2
}