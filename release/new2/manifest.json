{
  "name": "Spotify Search",
  "description": "Search Spotify artist, albums and tracks right from the URL bar of Chrome",
  "version": "2.1",
  "omnibox": { "keyword" : "spotify" },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "logo19.png"
  },
  "icons":{
	  "16":"logo16.png",
	  "48":"logo48.png",
	  "128":"logo128.png"
  },
  "permissions": [
    "contextMenus",
	"http://ws.spotify.com/"
  ],
  "manifest_version": 2
}